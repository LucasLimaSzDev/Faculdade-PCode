programa {
  funcao inicio() {
    
    inteiro n

    escreva("Digite um numero entre 1,2 e 3: ")
      leia(n)

    se(n==1){escreva("O numero digitado é 1.")}
    senao se (n==2){escreva("O numero digitado é 2.")}
    senao se (n==3){escreva("O numero digitado é 3.")}
    senao(escreva("O numero digitado precisa ser entre 1,2 ou 3."))
 

  }
}
